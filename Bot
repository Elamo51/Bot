    if (opcion=="Pasar de decimal a binario"):
        print("--------------------------------------------------------------")
        def binario(decimal):
            binario = ""
            while decimal // 2 != 0:
                binario = str(decimal % 2) + binario
                decimal = decimal //2
            return str(decimal) + binario

        numero = int(input("Introduce el numero que quieres convertir a binario: "))
        print("El numero binario es:")
        print(binario(numero))
        print("--------------------------------------------------------------")
    if (opcion=="pasar de decimal a binario"):
        print("--------------------------------------------------------------")
        def binario(decimal):
            binario = ""
            while decimal // 2 != 0:
                binario = str(decimal % 2) + binario
                decimal = decimal //2
            return str(decimal) + binario

        numero = int(input("Introduce el numero que quieres convertir a binario: "))
        print("El numero binario es:")
        print(binario(numero))
        print("--------------------------------------------------------------")
