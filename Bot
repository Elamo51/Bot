    if (opcion=="Juego Cho Han"):
        import random as rd
        billetera=10
        gana=0
        continuar="si"
        print("---Bienvenido a Cho Han---")
        print("Usted tiene ${} en la billetera".format(billetera))
        while billetera>0 and continuar=="si":
            apuesta=int(input("Ingrese apuesta: "))
            if apuesta <= billetera:
                dado1=rd.randint(1,6)
                dado2=rd.randint(1,6)
                total=dado1=dado2
                resto=total%2
                parimpar=input("Adivina ¿par o impar? ")
                print("Salió: {} + {} + {}". format(dado1,dado2,total))
            if resto==0 and parimpar=="par":
                print("Gano!!")
                billetera+=apuesta
                gana+=1
            elif resto!=0 and parimpar=="impar":
                print("Gano! :D")
                billetera+=apuesta
                gana=+1
            else:
                print("Perdió :'(")
                billetera-=apuesta
                print("Billetera: {}".format(billetera))
                print("--------------------------------------------------------------")
            if billetera!=0:
                continuar = input ("¿Desea seguir jugando?")
                print("--------------------------------------------------------------")
        else:
            print("La apuesta es mayor que el dinero que tiene en la billetera (si esto te aparece cuando terminas el juego no hagas ni caso :>")
            print("Usted ganó {} partidas".format(gana))
            print("Gracias por jugar :>")
            print("--------------------------------------------------------------")
    
