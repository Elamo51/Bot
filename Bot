    if (opcion=="Cuentame un chiste"):
        while opcion!=("Nah, en realidad no quiero"):
            print("Uno negro")
            print("Uno de internet")
            print("Nah, en realidad no quiero")
            opcion=input("Ingrese una opcion: ")
            
            if (opcion=="Uno negro"):
                opcion=input("Ingrese el numero del chiste que quieres que te diga, hay hasta el numero 6: ")
                if (opcion=="1"):
                    print("--------------------------------------------------------------")
                    print("¿Cual es la unica diferencia entre un violador y un seductor?")
                    print("La paciencia XDDDD")
                    print("Dime que no es verdad XDDDD")
                    print("--------------------------------------------------------------")
                if (opcion=="2"):
                    print("--------------------------------------------------------------")
                    print("Un hombre acude al hospital para un chequeo medico, tras un exaustivo examen, le diagnostican una enfermedad rara que le llevara a la muerte en poco tiempo.")
                    print("El paciente pregunta:")
                    print("-¿Que puedo hacer para curarme?")
                    print("El medico repsonde:")
                    print("+Te aconsejo baños de barro")
                    print("-¿Y con eso me curaré?")
                    print("+No pero te irás acostumbrando a la tierra")
                    print("--------------------------------------------------------------")
                if (opcion=="3"):
                    print("--------------------------------------------------------------")
                    print("La mañana despues de navidad, y los dos niños que viven en el barrio, salen al patio a jugar con sus nuevos regalos.")
                    print("Uno de los dos niños, juega con una peonza de madera, y el otro, con un Dron teledirigidocon camara y visor 3D")
                    print("El niño del dron se rie del otro niño con la lengua fuera y le dice:")
                    print("+Mi regalo es mejor, mi regalo es mejor...")
                    print("A lo que el otro niño responde:")
                    print("-Si, pero yo no tengo cáncer")
                    print("--------------------------------------------------------------")
                if (opcion=="4"):
                    print("--------------------------------------------------------------")
                    print("¿Que tiene dos patas y sangra?")
                    opcion=input("Venga dime: ")
                    if (opcion=="Ns"):
                        print("Medio perro :(")
                        print("--------------------------------------------------------------")
                    if (opcion=="ns"):
                        print("Medio perro :(")
                        print("---------------------------------------------------------------")
                if (opcion=="5"):
                    print("--------------------------------------------------------------")
